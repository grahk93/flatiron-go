<%= form_for @user do |f| %>
  <%= f.label :cohort %>
  <%= f.collection_select :cohort_id, Cohort.all, :id, :name %><br>
  <%= f.label :email %> 
  <%= f.text_field :email %><br>
  <%= f.label :user_name %> 
  <%= f.text_field :user_name %><br>
  <%= f.label :password %> 
  <%= f.password_field :password %><br>
  <%= f.submit "Sign Up" %> 
<%end%>